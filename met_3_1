#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main() {
	setlocale(LC_ALL, "Russian");
	int x, y;
	cin >> x >> y;
	if ((y >= -x) && (x * x + y * y <= 10 * 10)) {
		if ((y == -x) || (x * x + y * y == 10 * 10)) {
			cout << "На границе";
		}
		else {
			cout << "Да";
		}
	}
	else {
		cout << "Нет";
	}
}

