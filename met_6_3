#include <iostream>

using namespace std;
int main() {
	int n; cin >> n;
	int** a = new int*[n];
	for(int i = 0; i < n; i++)
		a[i] = new int[n];
 
	for (int i = 0; i < n; i++)
		for (int j = 0; j < n; j++)
			cin >> a[i][j];
 
	int cnt = 0;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (a[i][j] < 0) {
				cnt++;
				break;
			}
		}
	}
 
	if (cnt == n) {
		cout << "No";
	}
	else {
		cout << "Yes";
	}
 
}
