#include <iostream>
#include <cmath>
#include <iomanip>


using namespace std;

int main() {
	for (int i = 10; i < 100; i++) {
		if (abs(i % 10 - (i / 10)) <= 1)
			cout << i << endl;
	}
}

#include <iostream>
#include <cmath>
#include <iomanip>


using namespace std;
int main() {
		int i = 10;
			while (i < 100) {
				if (abs(i % 10 - (i / 10)) <= 1)
					cout << i << endl;
				i++;
		}
	}
	
	
	
#include <iostream>
#include <cmath>
#include <iomanip>


using namespace std;
int main() {
		int i = 10;
		do{
			if (abs(i % 10 - (i / 10)) <= 1)
				cout << i << endl;
			i++;
		
		} while (i < 100);
	}

