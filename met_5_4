#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
double e = 1e-9;
double func (double x) {
	int i = 1;
	double k = 1;
	double a = double(x)/3;
	while (a >= e) {
		k += a;
		a*=a;
		i++;
	}
	return k;
}
int main() {
	double h;
	cin >> h;
	for (double k = 0; k <= 0.9+e; k += h) {
		cout << k << "  " << func(k)<<endl;
	}
}
